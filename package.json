{
  "name": "medi-thanks",
  "homepage": "https://pb-j.github.io/frontline-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@popperjs/core": "^2.6.0",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "bootstrap-icons": "^1.2.2",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "^7.0.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "emoji-picker-react": "^3.2.4",
    "fs-extra": "3.0.1",
    "gh-pages": "^3.1.0",
    "moment": "^2.29.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.2",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.4.2",
    "react-emoji-textarea": "^2.0.1",
    "react-icons": "^4.1.0",
    "react-input-emoji": "^2.1.1",
    "react-router-dom": "^4.3.1",
    "resolve": "1.6.0",
    "source-map-loader": "^2.0.0",
    "url-loader": "0.6.2",
    "webpack": "^5.19.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "start": "webpack-dev-server ",
    "build": "webpack -p",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.{js,scss}' --write",
    "lint": "eslint . --ext .js",
    "deploy": "gh-pages --dist dist"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@webpack-cli/serve": "^1.2.2",
    "ajv": "^5.5.2",
    "babel-core": "6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "20.0.3",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "^4.3.0",
    "eslint": "^5.14.1",
    "eslint-config-react-app": "^3.0.7",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.21.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "20.0.4",
    "node-sass": "^4.9.3",
    "postcss": "^8.1.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "^4.0.2",
    "react-dev-utils": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.2.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "1.3.2"
  }
}
